generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql"
    url      = env("POSTGRES_URL")
}

model Member {
    id          String          @id @default(uuid())
    name        String
    about       String?
    photoUrl    String
    type        String?
    isActive    Boolean         @default(true)
    contactInfo MemberContact[]
}

model MemberContact {
    id       String @id @default(uuid())
    name     String
    snsId    String
    memberId String
    member   Member @relation(fields: [memberId], references: [id])
}
